# Makefile.in generated by hand.
# @configure_input@

CC = @CC@
CFLAGS = @CFLAGS@
CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
CXX = @CXX@
CXXLD = $(CXX)
CXXCPP = @CXXCPP@
CXXFLAGS = @CXXFLAGS@ -O3 -Wall -Wextra -fPIC
LDFLAGS = @LDFLAGS@ -pthread
LIBS = @LIBS@
MKDIR_P = @MKDIR_P@

EXEEXT = @EXEEXT@
OBJEXT = @OBJEXT@

$(shell $(MKDIR_P) bin)

all: 2048$(EXEEXT)

%$(EXEEXT): %.$(OBJEXT)
	$(CXXLD) $(CXXFLAGS) $(LDFLAGS) $^ $(LDLIBS) -o $@

%.$(OBJEXT) : %.cpp
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c -o $@ $<

clean:
	$(RM) -rf 2048$(EXEEXT) 2048$(OBJEXT)

.PHONY: all clean
